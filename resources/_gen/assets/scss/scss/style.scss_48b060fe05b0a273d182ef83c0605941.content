*,*::before,*::after{box-sizing:border-box}*{margin:0;padding:0}html,body{height:100%}body{line-height:1.5;-webkit-font-smoothing:antialiased}img,picture,video,canvas,svg{display:block;max-width:100%;height:auto}input,button,textarea,select{font:inherit}p,h1,h2,h3,h4,h5,h6{overflow-wrap:break-word}@font-face{font-family:yakucaltgothic;src:url(../../static/font/YakuCalt/YakuCaltGothic-R.woff)format("woff"),url(../../static/font/YakuCalt/YakuCaltMincho-R.otf)format("opentype");font-style:normal;font-weight:400;font-display:swap}@font-face{font-family:hummingbird;src:url(../../static/font/Hummingbird/Hummingbird.woff)format("woff");font-style:normal;font-weight:400;font-display:swap}@font-face{font-family:plusquam sans;src:url(../../static/font/PlusquamSans/PlusquamSans.woff)format("woff");font-style:normal;font-weight:400;font-display:swap}@font-face{font-family:plusquam sans;src:url(../../static/font/PlusquamSans/PlusquamSans-Bold.woff)format("woff");font-style:normal;font-weight:700;font-display:swap}html{font-variant-ligatures:contextual;-moz-font-feature-settings:"calt";-webkit-font-feature-settings:"calt";font-feature-settings:"calt"}body{background-color:#d2e0ac;color:#5b534d;font-family:yakucaltgothic,plusquam sans,zen kaku gothic new,-apple-system,BlinkMacSystemFont,avenir next,avenir,helvetica neue,helvetica,ubuntu,roboto,noto,segoe ui,arial,sans-serif;font-size:1.0625rem}h1,h2,h3,h4,h5,h6{font-family:plusquam sans,zen kaku gothic new,-apple-system,BlinkMacSystemFont,avenir next,avenir,helvetica neue,helvetica,ubuntu,roboto,noto,segoe ui,arial,sans-serif}a{text-decoration:underline;opacity:1;transition:opacity .15s ease-in}a:hover,a:focus{opacity:.5;transition:opacity .15s ease-in}a:active{opacity:.8;transition:opacity .15s ease-out}.url{word-break:break-all}